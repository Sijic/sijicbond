<!-- Tabs Selection -->
<script>

  $('.learn-nav_item').on('click', function() {

    if($(this).hasClass("photo")){
      $('.learn-tab-link.photo').triggerHandler('click');
    }
    
    if($(this).hasClass("gigapixel")){
      $('.learn-tab-link.gigapixel').triggerHandler('click');
    }

    else if($(this).hasClass("sharpen")){
      $('.learn-tab-link.sharpen').triggerHandler('click');
    }

    else if($(this).hasClass("denoise")){
      $('.learn-tab-link.denoise').triggerHandler('click');
    }

    else if($(this).hasClass("video")){
      $('.learn-tab-link.video').triggerHandler('click');
    }


  });
</script>

<!-- TRIM OF Text -->
<script> 

  $(".learn-feature_content-body-head .h4_new").not( ".featured" ).each (function () {
    if ($(this).text().length > 55)
      $(this).text($(this).text().substring(0,55) + '...');
  });
  
  $(".learn-feature_content-body .p-16_new").each (function () {
    if ($(this).text().length > 140)
      $(this).text($(this).text().substring(0,140) + '...');
  });
</script>
