<script>
jQuery(function($) {
    //Setting up the structure for each job posting
    function createJobs(_data) {

        //Checking for potential Lever source or origin parameters
        var pageUrl = window.location.href;
        var leverParameter = '';
        var trackingPrefix = '?lever-'

        if (pageUrl.indexOf(trackingPrefix) >= 0) {
            // Found Lever parameter
            var pageUrlSplit = pageUrl.split(trackingPrefix);
            leverParameter = '?lever-' + pageUrlSplit[1];
        } else {
            leverParameter = '?lever-source=TopazCareers'
        }

        // Grouping Jobs by Team
        function groupBy(data, fn) {
            return data.reduce(function(result, current) {
                result[fn(current)] = result[fn(current)] || [];
                result[fn(current)].push(current);
                return result;
            }, {})
        }
        let teams = groupBy(_data, (current) => current.categories.team);

        let eachTeam = Object.keys(teams);

        let jobsList = document.querySelector("#jobs-list");
        let teamNode = document.querySelector(".b2-listing-team");

        eachTeam.forEach((team) => {
            const teamWidget =
                teamNode.cloneNode(true)
            teamWidget.removeAttribute("id")
            teamWidget.querySelector(".b2-job-team").innerHTML = team;

            const postings = teams[team]

            const jobNode = teamWidget.querySelector(".b2-job")
            postings.forEach(posting => {
                const jobWidget = jobNode.cloneNode(true);
                jobWidget.removeAttribute("id")

                jobWidget.querySelector(".b2-job-title").innerHTML = posting.text;

                jobWidget.querySelector(".b2-job-location").innerHTML = posting.categories.location;

                jobWidget.querySelector(".b2-job-commitment").innerHTML = posting.categories.commitment;

                jobWidget.querySelector(".b2-job-link").href = posting.hostedUrl + leverParameter;

                teamWidget.appendChild(jobWidget)
            })

            jobsList.appendChild(teamWidget)
        })
    }

    $.ajax({
        dataType: "json",
        url: "https://api.lever.co/v0/postings/topazlabs?mode=json",
        success: function(data) {
            $('#jobs-list .fa-spin').hide();
            createJobs(data);

        }
    });
});
</script>

