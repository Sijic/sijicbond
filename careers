  jQuery(function($) {
    tippy('.tooltip-trigger', {
      animation: 'scale',
      duration: 200,
      arrow: true,
      delay: [0, 50],
      arrowType: 'sharp',
      theme: 'light',
      maxWidth: 350,
      allowHTML: true,
      placement: 'bottom'
    })

    const trialPopup = document.getElementById('trial-popup-container');

    if (trialPopup) {


