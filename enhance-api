<!-- Temp Trial Fix -->
<script>
  const trialButtonList = document.getElementsByClassName('trial-button');
  const trialTriggerList = document.getElementsByClassName('trial-popup-trigger');

  function enableTrial() {
    setTimeout(function (){
  
    if(trialButtonList.length > 0) {
      for(let i = 0; i < trialButtonList.length; i++) {
        trialButtonList[i].removeAttribute('disabled');
      }
    }

    }, 1000);
  }

  if(trialTriggerList.length > 0) {
    for(let i = 0; i < trialButtonList.length; i++) {
      trialTriggerList[i].addEventListener('click', enableTrial);
    }
  }


</script>
<script>
    const apiMonthlyPrice = document.getElementById('api-monthly-price');
    const apiTokens = document.getElementById('api-tokens');
    const apiTokenPrice = document.getElementById('api-token-price');
    
    const apiCartButton = document.getElementById('api-cart-button');
    
    let apiSelections = document.getElementsByClassName('api-grid-wrap');
    
    function apiGridSelect() {
        apiMonthlyPrice.innerHTML = this.dataset.apiMonthlyPrice;
        apiTokens.innerHTML = this.dataset.apiTokens;
        apiTokenPrice.innerHTML = this.dataset.apiTokenPrice;
        apiCartButton.href = this.dataset.apiLink
    }
    
    if(apiSelections.length > 0) {
        for(let i = 0; i < apiSelections.length; i++) {
            apiSelections[i].addEventListener("click", apiGridSelect);
        }
    }
</script></body></html>