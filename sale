	const detailsCreditAmt = document.getElementById("details-credit-amt");

	const heroProdPhoto = document.getElementById('bf24-hero-product-photo');
	const heroProdVideo = document.getElementById('bf24-hero-product-video');
	const heroProdGiga = document.getElementById('bf24-hero-product-giga');
	const heroProdPhotoOwn = document.getElementById('bf24-hero-product-photo-own');
	const heroProdVideoOwn = document.getElementById('bf24-hero-product-video-own');
	const heroProdGigaOwn = document.getElementById('bf24-hero-product-giga-own');
	const bundleSection = document.getElementById('bundle-section');
	const ownSection = document.getElementById('owned-section');
	const topBarPrice = document.getElementById('top-bar-price');
	const topBarLoginLink = document.getElementById('top-bar-login-link');
	const topBarLoginUser = document.getElementById('top-bar-login-user');
	const oneBigBundle = document.getElementById('one-big-bundle');
	const percentOffTag = document.getElementById('percent-off-tag');
	const dynamicHeadline = document.getElementById('dynamic-headline');
	const dynamicHeadlineSecondary = document.getElementById('dynamic-headline-secondary');
	const dynamicBundleDescript = document.getElementById('dynamic-bundle-descript');

	const sideUpgradeSection = document.getElementById('side-upgrade-section');
	const sideUpgradePhoto = document.getElementById('side-upgrade-photo');
	const sideUpgradeVideo = document.getElementById('side-upgrade-video');
	const sideUpgradeGiga = document.getElementById('side-upgrade-giga');

	const detailsPhotoBlock = document.getElementById('details-photo');
	const detailsVideoBlock = document.getElementById('details-video');
	const detailsGigaBlock = document.getElementById('details-giga');
	const detailsCreditBlock = document.getElementById('details-credits');

	const sideUpgradeLabel = document.getElementById('side-upgrade-label');
	const sideUpgradeImg = document.getElementById('side-upgrade-img');
	const sideUpgradeOrgPrice = document.getElementById('side-upgrade-org-price');
	const sideUpgradePrice = document.getElementById('side-upgrade-price');
	const sideUpgradeSave = document.getElementById('side-upgrade-save');
	const sideUpgradeDescript = document.getElementById('side-upgrade-descript');

	const sideCreditOrgPrice = document.getElementById('side-credit-org-price');
	const sideCreditPrice = document.getElementById('side-credit-price');
	const sideCreditSave = document.getElementById('side-credit-save');

	const newUserSection = document.getElementById('newuser-section');
	const existingUserSection = document.getElementById('existinguser-section');
	const heroCloudCreditSection = document.getElementById('hero-cloud-credits-section');
	const heroBundlePriceSection = document.getElementById('hero-bundle-price-section');
	const heroDeatilsSection = document.getElementById('hero-details-section');

	const starterPackPhoto = document.getElementById('starter-photo');
	const starterPackVideo = document.getElementById('starter-video');
	const starterPackGiga = document.getElementById('starter-giga');

	let appPhotoOwned = false;
	let appPhotoExpired = false;
	let appPhotoSubscribed = false;
	let appPhotoPlus12Window = false;

	let appVideoOwned = false;
	let appVideoExpired = false;
	let appVideoSubscribed = false;
	let appVideoPlus12Window = false;

	let appGigaOwned = false;
	let appGigaExpired = false;
	let appGigaSubscribed = false;
	let appGigaPlus12Window = false;

	let loggedIn = false;

	if(debug) {
		console.log('variables init');
	}

	function creditSelect() {
		if(debug) {
			console.log('credit selected');
		}

		selectedCreditAmt = this.dataset.credits;
		selectedCreditPrice = this.dataset.price;
		selectedCreditOrgPrice = this.dataset.orgPrice;
		selectedCreditId = this.dataset.packId;

		recalcBundle();
	}

	function recalcBundle() {

		bundlePrice = 0;
		bundleOrgPrice = 0;

		bundlePrice = parseInt(selectedPhotoPrice) + parseInt(selectedVideoPrice) + parseInt(selectedGigaPrice) + parseInt(selectedCreditPrice);
		bundleOrgPrice = parseInt(selectedPhotoOrgPrice) + parseInt(selectedVideoOrgPrice) + parseInt(selectedGigaOrgPrice) + parseInt(selectedCreditOrgPrice);
		percentOff = 1 - parseFloat(bundlePrice) / parseFloat(bundleOrgPrice);

		adjustBundleUI();
	}

	function adjustBundleUI() {
		bundlePriceElement.innerHTML = "$" + bundlePrice;
		bundleOrgPriceElement.innerHTML = "$" + bundleOrgPrice;

		if(expiredDayBundle) {
			percentOffTag.innerHTML = "Save $162";
		} else {
			percentOffTag.innerHTML = Math.round(parseFloat(percentOff) * 100) + "% Off";
		}
		
		topBarPrice.innerHTML = "$" + bundlePrice;
		detailsCreditAmt.innerHTML = selectedCreditAmt;

		if(debug) {
			console.log("selected photo price: " + selectedPhotoPrice + " + original: " + selectedPhotoOrgPrice + ", selected video price: " + selectedVideoPrice + " + original: " + selectedVideoOrgPrice + ", selected gigapixel price: " + selectedGigaPrice + " + original: " + selectedGigaOrgPrice);
		}

		//adjust button numbers
		bundleBuyButton.querySelector('.tnb-button-content:nth-child(1) .tnb-link-text:nth-child(2)').innerHTML = "$" + bundlePrice;
		bundleBuyButton.querySelector('.tnb-button-content:nth-child(2) .tnb-link-text:nth-child(2)').innerHTML = "$" + bundlePrice;

		//adjust button link
		if(!bundlePurchased) {
			if(selectedCreditId == "none") {
				bundleBuyButton.href = "https://topazlabs.com/cart?add-to-cart=4566735&clear-cart=true";
			} else {
				bundleBuyButton.href = `https://topazlabs.com/cart?add-to-cart=4566735&credit_pack=${selectedCreditId}&clear-cart=true`;
			}
		}
	}

	function adjustLoggedIn() {
		if(!loggedIn) {
			existingUserSection.style.display = 'none';
			newUserSection.style.display = 'block';
			ownSection.style.display = 'none';
			recalcBundle();
			return;
		}

		if(debug) {
			console.log(`${appPhotoOwned}, ${appPhotoExpired}, ${appVideoOwned}, ${appVideoExpired}, ${appGigaOwned}, ${appGigaExpired}`);
		}

		sideUpgradeSection.classList.add('hide');
		sideUpgradePhoto.classList.add('hide');
		sideUpgradeVideo.classList.add('hide');
		sideUpgradeGiga.classList.add('hide');

		existingUserSection.style.display = 'none';
		newUserSection.style.display = 'none';

		if(appPhotoOwned || appVideoOwned || appGigaOwned) {
			existingUserSection.style.display = 'block';

			//video starter change - delete line, adding check for !owned below
			//newUserSection.style.display = 'none';

			if(debug) {
				console.log('existing user')
			}
		}

		if(!appPhotoOwned || !appVideoOwned || !appGigaOwned) {
			newUserSection.style.display = 'block';

			if(!appPhotoOwned) {
				starterPackPhoto.style.display = 'flex';
			}	else { starterPackPhoto.style.display = 'none'; }

			if(!appVideoOwned) {
				starterPackVideo.style.display = 'flex';
			}	else { starterPackVideo.style.display = 'none'; }

			if(!appGigaOwned) {
				starterPackGiga.style.display = 'flex';
			}	else { starterPackGiga.style.display = 'none'; }
		}

		if(expiredDayBundle && !bundlePurchased){
			//set bundle
			ownSection.style.display = 'none';
			bundleSection.style.display = 'flex';

			heroProdPhoto.style.display = 'flex';
			heroProdVideo.style.display = 'flex';
			heroProdGiga.style.display = 'flex';

			heroProdPhoto.querySelector('.tnb-h-xsmall').innerHTML = 'Photo AI - Auto-renew';
			heroProdPhoto.querySelector('.bf24-org-price').innerHTML = `$${renewalPhotoOrgPrice}`;
			heroProdPhoto.querySelector('.bf24-additional-info').style.display = 'block';
			detailsPhotoBlock.querySelector('.tnb-h-xsmall').innerHTML = 'Photo AI - Auto-renew';
			detailsPhotoBlock.style.display = 'block';
			selectedPhotoPrice = renewalPhotoPrice;
			selectedPhotoOrgPrice = renewalPhotoOrgPrice;

			heroProdVideo.querySelector('.tnb-h-xsmall').innerHTML = 'Video AI - Auto-renew';
			heroProdVideo.querySelector('.bf24-org-price').innerHTML = `$${renewalVideoOrgPrice}`;
			heroProdVideo.querySelector('.bf24-additional-info').style.display = 'block';
			detailsVideoBlock.querySelector('.tnb-h-xsmall').innerHTML = 'Video AI - Auto-renew';
			detailsVideoBlock.style.display = 'block';
			selectedVideoPrice = renewalVideoPrice;
			selectedVideoOrgPrice = renewalVideoOrgPrice;

			heroProdGiga.querySelector('.tnb-h-xsmall').innerHTML = 'Gigapixel - Auto-renew';
			heroProdGiga.querySelector('.bf24-org-price').innerHTML = `$${renewalGigaOrgPrice}`;
			heroProdGiga.querySelector('.bf24-additional-info').style.display = 'block';
			detailsGigaBlock.querySelector('.tnb-h-xsmall').innerHTML = 'Gigapixel - Auto-renew';
			detailsGigaBlock.style.display = 'block';
			selectedGigaPrice = renewalGigaPrice;
			selectedGigaOrgPrice = renewalGigaOrgPrice;

			sideUpgradePhoto.classList.remove('hide');
			sideUpgradeSection.classList.remove('hide');
			sideUpgradePhoto.querySelector('.tnb-h-xsmall').innerHTML = "Photo AI - Auto-renew";

			sideUpgradeVideo.classList.remove('hide');
			sideUpgradeSection.classList.remove('hide');
			sideUpgradeVideo.querySelector('.tnb-h-xsmall').innerHTML = "Video AI - Auto-renew";

			sideUpgradeGiga.classList.remove('hide');
			sideUpgradeSection.classList.remove('hide');
			sideUpgradeGiga.querySelector('.tnb-h-xsmall').innerHTML = "Gigapixel - Auto-renew";

			bundleScenario = 'ownAll60';

			dynamicCopy();
			recalcBundle();
			sideUpgradeReset();

			return
		}

		//photo
		if(appPhotoExpired) {
			selectedPhotoPrice = upgradePhotoPrice;
			selectedPhotoOrgPrice = upgradePhotoOrgPrice;

			heroProdPhoto.style.display = 'flex';
			heroProdPhotoOwn.style.display = 'none';
			heroProdPhoto.querySelector('.tnb-h-xsmall').innerHTML = 'Photo AI - Upgrade';
			heroProdPhoto.querySelector('.bf24-org-price').innerHTML = `$${upgradePhotoOrgPrice}`;
			detailsPhotoBlock.querySelector('.tnb-h-xsmall').innerHTML = 'Photo AI - Upgrade';
			detailsPhotoBlock.style.display = 'block';

			heroProdPhoto.querySelector('.bf24-additional-info').style.display = 'block';

			sideUpgradePhoto.classList.remove('hide');
			sideUpgradeSection.classList.remove('hide');
			sideUpgradePhoto.querySelector('.tnb-h-xsmall').innerHTML = "Photo AI - Upgrade";
		} else if(appPhotoOwned) {
			selectedPhotoPrice = 0;
			selectedPhotoOrgPrice = 0;

			heroProdPhoto.style.display = 'none';
			heroProdPhotoOwn.style.display = 'flex';
			detailsPhotoBlock.style.display = 'none';

			heroProdPhotoOwn.querySelector('.bf24-additional-info').style.display = 'block';

			if(!appPhotoPlus12Window && !appPhotoSubscribed) {
				sideUpgradePhoto.classList.remove('hide');
				sideUpgradeSection.classList.remove('hide');
				sideUpgradePhoto.querySelector('.tnb-h-xsmall').innerHTML = "Photo AI - Add 12 Months";
			}
			
		}	else {
			selectedPhotoPrice = appPhotoPrice;
			selectedPhotoOrgPrice = appPhotoOrgPrice;


			heroProdPhoto.style.display = 'flex';
			heroProdPhotoOwn.style.display = 'none';
			heroProdPhoto.querySelector('.tnb-h-xsmall').innerHTML = 'Photo AI 3';
			heroProdPhoto.querySelector('.bf24-org-price').innerHTML = `$${appPhotoOrgPrice}`;
			detailsPhotoBlock.querySelector('.tnb-h-xsmall').innerHTML = 'Photo AI 3';
			detailsPhotoBlock.style.display = 'block';

			heroProdPhoto.querySelector('.bf24-additional-info').style.display = 'none';
			heroProdPhotoOwn.querySelector('.bf24-additional-info').style.display = 'none';
		}

		//video
		if(appVideoExpired) {
			selectedVideoPrice = upgradeVideoPrice;
			selectedVideoOrgPrice = upgradeVideoOrgPrice;

			heroProdVideo.style.display = 'flex';
			heroProdVideoOwn.style.display = 'none';
			heroProdVideo.querySelector('.tnb-h-xsmall').innerHTML = 'Video AI - Upgrade';
			heroProdVideo.querySelector('.bf24-org-price').innerHTML = `$${upgradeVideoOrgPrice}`;
			detailsVideoBlock.querySelector('.tnb-h-xsmall').innerHTML = 'Video AI - Upgrade';
			detailsVideoBlock.style.display = 'block';

			heroProdVideo.querySelector('.bf24-additional-info').style.display = 'block';

			sideUpgradeVideo.classList.remove('hide');
			sideUpgradeSection.classList.remove('hide');
			sideUpgradeVideo.querySelector('.tnb-h-xsmall').innerHTML = "Video AI - Upgrade";
		} else if(appVideoOwned) {
			selectedVideoPrice = 0;
			selectedVideoOrgPrice = 0;

			heroProdVideo.style.display = 'none';
			heroProdVideoOwn.style.display = 'flex';
			detailsVideoBlock.style.display = 'none';

			heroProdVideoOwn.querySelector('.bf24-additional-info').style.display = 'block';

			if(!appVideoPlus12Window && !appVideoSubscribed) {
				sideUpgradeVideo.classList.remove('hide');
				sideUpgradeSection.classList.remove('hide');
				sideUpgradeVideo.querySelector('.tnb-h-xsmall').innerHTML = "Video AI - Add 12 Months";
			}
			
		}	else {
			selectedVideoPrice = appVideoPrice;
			selectedVideoOrgPrice = appVideoOrgPrice;

			heroProdVideo.style.display = 'flex';
			heroProdVideoOwn.style.display = 'none';
			heroProdVideo.querySelector('.tnb-h-xsmall').innerHTML = 'Video AI 5';
			heroProdVideo.querySelector('.bf24-org-price').innerHTML = `$${appVideoOrgPrice}`;
			detailsVideoBlock.querySelector('.tnb-h-xsmall').innerHTML = 'Video AI 5';
			detailsVideoBlock.style.display = 'block';

			heroProdVideo.querySelector('.bf24-additional-info').style.display = 'none';
			heroProdVideoOwn.querySelector('.bf24-additional-info').style.display = 'none';
		}


		//Gigapixel
		if(appGigaExpired) {
			selectedGigaPrice = upgradeGigaPrice;
			selectedGigaOrgPrice = upgradeGigaOrgPrice;

			heroProdGiga.style.display = 'flex';
			heroProdGigaOwn.style.display = 'none';
			heroProdGiga.querySelector('.tnb-h-xsmall').innerHTML = 'Gigapixel - Upgrade';
			heroProdGiga.querySelector('.bf24-org-price').innerHTML = `$${upgradeGigaOrgPrice}`;
			detailsGigaBlock.querySelector('.tnb-h-xsmall').innerHTML = 'Gigapixel - Upgrade';
			detailsGigaBlock.style.display = 'block';

			heroProdGiga.querySelector('.bf24-additional-info').style.display = 'block';

			sideUpgradeGiga.classList.remove('hide');
			sideUpgradeSection.classList.remove('hide');
			sideUpgradeGiga.querySelector('.tnb-h-xsmall').innerHTML = "Gigapixel - Upgrade";

		} else if(appGigaOwned) {
			selectedGigaPrice = 0;
			selectedGigaOrgPrice = 0;

			heroProdGiga.style.display = 'none';
			heroProdGigaOwn.style.display = 'flex';
			detailsGigaBlock.style.display = 'none';

			if(debug) {
    			console.log("gigapixel test not showing - 3");
    		}

			heroProdGigaOwn.querySelector('.bf24-additional-info').style.display = 'block';

			if(!appGigaPlus12Window && !appGigaSubscribed) {
				sideUpgradeGiga.classList.remove('hide');
				sideUpgradeSection.classList.remove('hide');
				sideUpgradeGiga.querySelector('.tnb-h-xsmall').innerHTML = "Gigapixel - Add 12 Months";
			}
		}	else {
			selectedGigaPrice = appGigaPrice;
			selectedGigaOrgPrice = appGigaOrgPrice;

			heroProdGiga.style.display = 'flex';
			heroProdGigaOwn.style.display = 'none';
			heroProdGiga.querySelector('.tnb-h-xsmall').innerHTML = 'Gigapixel 8';
			heroProdGiga.querySelector('.bf24-org-price').innerHTML = `$${appGigaOrgPrice}`;
			detailsGigaBlock.querySelector('.tnb-h-xsmall').innerHTML = 'Gigapixel 8';
			detailsGigaBlock.style.display = 'block';

			heroProdGiga.querySelector('.bf24-additional-info').style.display = 'none';
			heroProdGigaOwn.querySelector('.bf24-additional-info').style.display = 'none';
		}


		// new user
		if(!appPhotoOwned && !appVideoOwned && !appGigaOwned) {
			if(debug) {
				console.log('new user');
			}

			existingUserSection.style.display = 'none';
			newUserSection.style.display = 'block';
			ownSection.style.display = 'none';

			bundleScenario = 'ownNothing';
		} 

		// owns all and all not expired (can get free credits)
		else if((appPhotoOwned && !appPhotoExpired) && (appVideoOwned && !appVideoExpired) && (appGigaOwned && !appGigaExpired)) {
			newUserSection.style.display = 'none';
			ownSection.style.display = 'flex';
			bundleSection.style.display = 'none';
			oneBigBundle.style.display = 'none';

			bundleScenario = 'ownAllFree';
		} 

		// if own and not expired 1 or 2 products
		else if((appPhotoOwned && !appPhotoExpired) || (appVideoOwned && !appVideoExpired) || (appGigaOwned && !appGigaExpired)) {
			bundleSection.style.display = 'flex';
			ownSection.style.display = 'flex';

			bundleScenario = 'mix';
		}

		// all expired
		else {
			newUserSection.style.display = 'none';
			bundleSection.style.display = 'flex';
			ownSection.style.display = 'none';

			bundleScenario = 'mix';
		}

		if(bundlePurchased) {
			bundleScenario = 'alreadyPurchased';
			heroCloudCreditSection.style.display = 'none';
			heroBundlePriceSection.style.display = 'none';
			heroDeatilsSection.style.display = 'none';
			bundleBuyButton.querySelector('.tnb-button-content:nth-child(1) .tnb-link-text:nth-child(2)').style.display = 'none';
			bundleBuyButton.querySelector('.tnb-button-content:nth-child(2) .tnb-link-text:nth-child(2)').style.display = 'none';
			bundleBuyButton.querySelector('.tnb-button-content:nth-child(1) .tnb-link-text:nth-child(1)').innerHTML = 'Shop the sale';
			bundleBuyButton.querySelector('.tnb-button-content:nth-child(2) .tnb-link-text:nth-child(1)').innerHTML = 'Shop the sale';
			bundleBuyButton.href = "https://www.topazlabs.com/black-friday-2024#more";
		}

		dynamicCopy();
		recalcBundle();
		sideUpgradeReset();
	}

	function changeStatus(product, status) {
		loggedIn = true;

		if(product == 'photo' && status == 'not-owned') {
			appPhotoOwned = false;
			appPhotoExpired = false;
		} else if(product == 'photo' && status == 'expired') {
			appPhotoOwned = true;
			appPhotoExpired = true;
		} else if(product == 'photo' && status == 'owned') {
			appPhotoOwned = true;
			appPhotoExpired = false;
		}

		if(product == 'video' && status == 'not-owned') {
			appVideoOwned = false;
			appVideoExpired = false;
		} else if(product == 'video' && status == 'expired') {
			appVideoOwned = true;
			appVideoExpired = true;
		} else if(product == 'video' && status == 'owned') {
			appVideoOwned = true;
			appVideoExpired = false;
		}

		if(product == 'giga' && status == 'not-owned') {
			appGigaOwned = false;
			appGigaExpired = false;
		} else if(product == 'giga' && status == 'expired') {
			appGigaOwned = true;
			appGigaExpired = true;
		} else if(product == 'giga' && status == 'owned') {
			appGigaOwned = true;
			appGigaExpired = false;
		}

		adjustLoggedIn();
	}

	function sideUpgradeSelect() {
		sideUpgradeLabel.innerHTML = `${this.dataset.product} - Upgrade`;
		sideUpgradeImg.src = this.dataset.img;
		sideUpgradeOrgPrice.innerHTML = this.dataset.orgPrice;
		sideUpgradePrice.innerHTML = this.dataset.price;
		sideUpgradeSave.innerHTML = this.dataset.save;

		upgradeBuyBotton.querySelector('.tnb-button-content:nth-child(1) .tnb-link-text:nth-child(2)').innerHTML = this.dataset.price;
		upgradeBuyBotton.querySelector('.tnb-button-content:nth-child(2) .tnb-link-text:nth-child(2)').innerHTML = this.dataset.price;

		//adjust button link
		upgradeBuyBotton.href = this.dataset.link;
	}

	function creditPackSelect() {
		sideCreditOrgPrice.innerHTML = this.dataset.orgPrice;
		sideCreditPrice.innerHTML = this.dataset.price;
		sideCreditSave.innerHTML = this.dataset.save;

		sideCreditBuyButton.querySelector('.tnb-button-content:nth-child(1) .tnb-link-text:nth-child(2)').innerHTML = this.dataset.price;
		sideCreditBuyButton.querySelector('.tnb-button-content:nth-child(2) .tnb-link-text:nth-child(2)').innerHTML = this.dataset.price;

		//adjust button link
		sideCreditBuyButton.href = `https://topazlabs.com/cart?add-to-cart=${this.dataset.packId}&clear-cart=true`;
	}

	//init all selector buttons
	const selectorButtons = document.querySelectorAll("[data-selector='cc-hero']");
	const selectorButtonsSideUpgrade = document.querySelectorAll("[data-selector='side-sale-upgrade']");
	const selectorButtonsCreditPacks = document.querySelectorAll("[data-selector='side-cc']");

	if(selectorButtons.length > 0) {
		for(let i = 0; i < selectorButtons.length; i++) {
			selectorButtons[i].addEventListener('click',creditSelect);
		}
	}

	if(selectorButtonsSideUpgrade.length > 0) {
		for(let i = 0; i < selectorButtonsSideUpgrade.length; i++) {
			selectorButtonsSideUpgrade[i].addEventListener('click',sideUpgradeSelect);
		}
	}

	if(selectorButtonsCreditPacks.length > 0) {
		for(let i = 0; i < selectorButtonsCreditPacks.length; i++) {
			selectorButtonsCreditPacks[i].addEventListener('click',creditPackSelect);
		}
	}

	function sideUpgradeReset() {
		if(debug) {
			console.log('side upgrade reset');
		}

		let allWraps = document.querySelectorAll(".tnb-selector-wrap > [data-selector='side-sale-upgrade']");

		if(debug) {
			console.log(allWraps);
		}

		if(allWraps.length > 0) {
			for(let i = 0; i < allWraps.length; i++) {
				allWraps[i].classList.remove('selected');
			}

			for(let i = 0; i < allWraps.length; i++) {
				if(allWraps[i].parentElement.classList.contains('hide')){
					if(debug) {
						console.log(`${allWraps[i].parentElement} contains hide`);
					}
				} else {
					if(debug) {
						console.log(`${allWraps[i].parentElement} doesn't contains hide`);
					}

					allWraps[i].classList.add('selected');

					sideUpgradeLabel.innerHTML = `${allWraps[i].dataset.product} - Upgrade`;
					sideUpgradeImg.src = allWraps[i].dataset.img;
					sideUpgradeOrgPrice.innerHTML = allWraps[i].dataset.orgPrice;
					sideUpgradePrice.innerHTML = allWraps[i].dataset.price;
					sideUpgradeSave.innerHTML = allWraps[i].dataset.save;

					upgradeBuyBotton.querySelector('.tnb-button-content:nth-child(1) .tnb-link-text:nth-child(2)').innerHTML = allWraps[i].dataset.price;
					upgradeBuyBotton.querySelector('.tnb-button-content:nth-child(2) .tnb-link-text:nth-child(2)').innerHTML = allWraps[i].dataset.price;

					//adjust button link
					upgradeBuyBotton.href = allWraps[i].dataset.link;

					break;
				}
			}

		}

		if(debug) {
			console.log(allWraps);
		}
	}

	function dynamicCopy() {
		switch(bundleScenario) {
		case 'ownAll60':
			dynamicHeadline.innerHTML = "Re-up your bundle.";
			dynamicHeadlineSecondary.innerHTML = "Best deal of the year.";
			dynamicBundleDescript.innerHTML = "<span class='tnb-color-white'>Your upgrades are ending soon, but you’ve got a Black Friday auto-upgrade deal.</span> Turn on auto-upgrades to lock in this exclusive pricing and get 200 free Cloud Credits, too. No charge today.";
			break;
		case 'ownAllFree':
			dynamicHeadline.innerHTML = "You have all the apps!";
			dynamicHeadlineSecondary.innerHTML = "Have 200 Cloud Credits on us.";
			dynamicBundleDescript.innerHTML = "<span class='tnb-color-white'>Yep, 200 Free Cloud Credits.</span> Thanks for going all-in on Topaz apps. To claim your free credits, select “Buy” below. If you’re looking for more credits, you can get them at 50-60% off , too!";
			break;
		case 'alreadyPurchased':
			dynamicHeadline.innerHTML = 'Sweet—you’ve got the bundle.';
			dynamicHeadlineSecondary.innerHTML = 'Shop more below.';
			dynamicBundleDescript.innerHTML = "<span class='tnb-color-white'>Welcome back!</span> Now that you’ve got the apps, check out the Cloud Credit packs below for lightning-fast rendering in Gigapixel.";
			break;
		case 'ownNothing':
			dynamicHeadline.innerHTML = 'Bundle up.';
			dynamicHeadlineSecondary.innerHTML = 'Best deal of the year.';
			dynamicBundleDescript.innerHTML = "<span class='tnb-color-white'>Join over 1 million users.</span> Professional-grade photo and video editing powered by AI.";
			break;
		case 'mix':
			dynamicHeadline.innerHTML = 'Complete your collection.';
			dynamicHeadlineSecondary.innerHTML = 'Best deal of the year.';
			dynamicBundleDescript.innerHTML = "<span class='tnb-color-white'>You're so close to having it all.</span> Pricing shown is built for you based on the apps you already own. So you can have it all, and at Black Friday prices.";
			break;
		default:
			console.log('no case found');
		}
	}

	function getDaysDifference(isoDate1, isoDate2) {
  		const date1 = new Date(isoDate1);
  		const date2 = new Date(isoDate2);

  		// Calculate the difference in milliseconds
  		const timeDifference = date2.getTime() - date1.getTime();

  		// Convert milliseconds to days
  		const daysDifference = Math.round(timeDifference / (1000 * 60 * 60 * 24));

  		return daysDifference;
	}

	if(debug) {
		console.log(`test day differece: ${getDaysDifference("2024-01-01","2024-01-04")}`);
	}

