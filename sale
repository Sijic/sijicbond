    				heroProdPhotoOwn.querySelector('.tnb-p-legal').innerHTML = `Expired ${expireDate.toLocaleDateString()}`;
    				sideUpgradePhoto.querySelector('.tnb-p-legal').innerHTML = `Expired ${expireDate.toLocaleDateString()}`;
    			} else {
    				heroProdPhoto.querySelector('.tnb-p-legal').innerHTML = `Upgrades expire ${expireDate.toLocaleDateString()}`;
    				heroProdPhotoOwn.querySelector('.tnb-p-legal').innerHTML = `Upgrades expire ${expireDate.toLocaleDateString()}`;
    				sideUpgradePhoto.querySelector('.tnb-p-legal').innerHTML = `Upgrades expire ${expireDate.toLocaleDateString()}`;

    				if(debug) {
    					let todayTest = new Date().toLocaleDateString();
						todayTest = new Date(todayTest);
    					console.log("Photo AI expires in " + getDaysDifference(todayTest,expireDate) + "days");
    				}
    			}

    			if(getDaysDifference(data.photo.expired_date, todayNextYear) <= 0) {
    				appPhotoPlus12Window = true;
    			}

    			if(debug) {
    				console.log("12 month test: " + getDaysDifference(data.photo.expired_date, todayNextYear));
    				let todayNextYearFormat = todayNextYear.toLocaleDateString();
    				console.log("today's date, next year: " + todayNextYearFormat);
    			}
    		} else if(!appPhotoSubscribed) {
    			if(appPhotoExpired){
    				heroProdPhoto.querySelector('.tnb-p-legal').innerHTML = `Expired`;
    				heroProdPhotoOwn.querySelector('.tnb-p-legal').innerHTML = `Expired`;
    				sideUpgradePhoto.querySelector('.tnb-p-legal').innerHTML = `Expired`;
    			} else {
    				heroProdPhoto.querySelector('.tnb-p-legal').innerHTML = `Active`;
    				heroProdPhotoOwn.querySelector('.tnb-p-legal').innerHTML = `Active`;
    				sideUpgradePhoto.querySelector('.tnb-p-legal').innerHTML = `Active`;
    			}
    		} else {
    			heroProdPhoto.querySelector('.tnb-p-legal').innerHTML = `Upgrades auto-renewing`;
    			heroProdPhotoOwn.querySelector('.tnb-p-legal').innerHTML = `Upgrades auto-renewing`;
    			sideUpgradePhoto.querySelector('.tnb-p-legal').innerHTML = `Upgrades auto-renewing`;
    		}

    		if(data.video.expired_date && !appVideoSubscribed) {
    			let expireDate = new Date(data.video.expired_date);

    			if(appVideoExpired){
    				heroProdVideo.querySelector('.tnb-p-legal').innerHTML = `Expired ${expireDate.toLocaleDateString()}`;
    				heroProdVideoOwn.querySelector('.tnb-p-legal').innerHTML = `Expired ${expireDate.toLocaleDateString()}`;
    				sideUpgradeVideo.querySelector('.tnb-p-legal').innerHTML = `Expired ${expireDate.toLocaleDateString()}`;
    			} else {
    				heroProdVideo.querySelector('.tnb-p-legal').innerHTML = `Upgrades expire ${expireDate.toLocaleDateString()}`;
    				heroProdVideoOwn.querySelector('.tnb-p-legal').innerHTML = `Upgrades expire ${expireDate.toLocaleDateString()}`;
    				sideUpgradeVideo.querySelector('.tnb-p-legal').innerHTML = `Upgrades expire ${expireDate.toLocaleDateString()}`;
    			}

    			if(getDaysDifference(data.video.expired_date, todayNextYear) <= 0) {
    				appVideoPlus12Window = true;
    			}
    		} else if(!appVideoSubscribed) {
    			if(appVideoExpired){
    				heroProdVideo.querySelector('.tnb-p-legal').innerHTML = `Expired`;
    				heroProdVideoOwn.querySelector('.tnb-p-legal').innerHTML = `Expired`;
    				sideUpgradeVideo.querySelector('.tnb-p-legal').innerHTML = `Expired`;
    			} else {
    				heroProdVideo.querySelector('.tnb-p-legal').innerHTML = `Active`;
    				heroProdVideoOwn.querySelector('.tnb-p-legal').innerHTML = `Active`;
    				sideUpgradeVideo.querySelector('.tnb-p-legal').innerHTML = `Active`;
    			}
    		} else {
    			heroProdVideo.querySelector('.tnb-p-legal').innerHTML = `Upgrades auto-renewing`;
    			heroProdVideoOwn.querySelector('.tnb-p-legal').innerHTML = `Upgrades auto-renewing`;
    			sideUpgradeVideo.querySelector('.tnb-p-legal').innerHTML = `Upgrades auto-renewing`;
    		}

    		if(data.gigapixel.expired_date && !appGigaSubscribed) {
    			let expireDate = new Date(data.gigapixel.expired_date);

    			if(debug) {
    				console.log("gigapixel test not showing");
    			}

    			if(appGigaExpired){
    				heroProdGiga.querySelector('.tnb-p-legal').innerHTML = `Expired ${expireDate.toLocaleDateString()}`;
    				heroProdGigaOwn.querySelector('.tnb-p-legal').innerHTML = `Expired ${expireDate.toLocaleDateString()}`;
    				sideUpgradeGiga.querySelector('.tnb-p-legal').innerHTML = `Expired ${expireDate.toLocaleDateString()}`;
    			} else {
    				heroProdGiga.querySelector('.tnb-p-legal').innerHTML = `Upgrades expire ${expireDate.toLocaleDateString()}`;
    				heroProdGigaOwn.querySelector('.tnb-p-legal').innerHTML = `Upgrades expire ${expireDate.toLocaleDateString()}`;
    				sideUpgradeGiga.querySelector('.tnb-p-legal').innerHTML = `Upgrades expire ${expireDate.toLocaleDateString()}`;

    				if(debug) {
    					console.log("gigapixel test not showing - 2");
    				}
