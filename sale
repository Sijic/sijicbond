    			}
    		} else if(!appGigaSubscribed) {
    			if(appGigaExpired){
    				heroProdGiga.querySelector('.tnb-p-legal').innerHTML = `Expired`;
    				heroProdGigaOwn.querySelector('.tnb-p-legal').innerHTML = `Expired`;
    				sideUpgradeGiga.querySelector('.tnb-p-legal').innerHTML = `Expired`;
    			} else {
    				heroProdGiga.querySelector('.tnb-p-legal').innerHTML = `Active`;
    				heroProdGigaOwn.querySelector('.tnb-p-legal').innerHTML = `Active`;
    				sideUpgradeGiga.querySelector('.tnb-p-legal').innerHTML = `Active`;
    			}
    		} else {
    			heroProdGiga.querySelector('.tnb-p-legal').innerHTML = `Upgrades auto-renewing`;
    			heroProdGigaOwn.querySelector('.tnb-p-legal').innerHTML = `Upgrades auto-renewing`;
    			sideUpgradeGiga.querySelector('.tnb-p-legal').innerHTML = `Upgrades auto-renewing`;
    		}

    		//check if all are within number of days expired
    		const dateToCheck = '2025-01-20';

    		if(debug) {
    			console.log("photo expired differece: " + getDaysDifference(data.photo.expired_date,dateToCheck));
    			console.log("video expired differece: " + getDaysDifference(data.video.expired_date,dateToCheck));
    			console.log("gigapixel expired differece: " + getDaysDifference(data.gigapixel.expired_date,dateToCheck));
    		}

    		if(!appPhotoExpired && (getDaysDifference(data.photo.expired_date,dateToCheck) <= 65 ) && (getDaysDifference(data.photo.expired_date,dateToCheck) >= 0 ) && !appVideoExpired && (0 <= getDaysDifference(data.video.expired_date,dateToCheck) <= 65) && (getDaysDifference(data.video.expired_date,dateToCheck) >= 0 )  && !appGigaExpired && (0 <= getDaysDifference(data.gigapixel.expired_date,dateToCheck) <= 65) && (getDaysDifference(data.gigapixel.expired_date,dateToCheck) >= 0 )) {

    			if(debug) {
    				console.log("65 day bundle elligible");
    			}

    			if(!appPhotoSubscribed && !appVideoSubscribed && !appGigaSubscribed) {
