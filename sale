				console.log("photo auto-renewing: " + appPhotoSubscribed);
			}

			appVideoOwned = data.video.owned;
			appVideoExpired = data.video.expired;
			appVideoSubscribed = data.video.autorenew;
			if(debug) {
				console.log("video auto-renewing: " + appVideoSubscribed);
			}

			appGigaOwned = data.gigapixel.owned;
			appGigaExpired = data.gigapixel.expired;
			appGigaSubscribed = data.gigapixel.autorenew;
			if(debug) {
				console.log("gigapixel auto-renewing: " + appGigaSubscribed);
			}

			//if bundle has already been purchased
			if(data.ultimate_bundle.already_bought) {
				bundlePurchased = data.ultimate_bundle.already_bought;
			}

			if(debug) {
				console.log("bundle already purchased: " + bundlePurchased);
			}

			let today = new Date().toLocaleDateString();
			today = new Date(today);

			if(debug) {
				console.log("today's date: " + today);
			}
			
			let todayNextYear = new Date(today.setFullYear(today.getFullYear() + 1));

