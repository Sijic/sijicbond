document.addEventListener("DOMContentLoaded", function() {
  var lazyVideos = [].slice.call(document.querySelectorAll("video.lazy"));

  if ("IntersectionObserver" in window) {
    var lazyVideoObserver = new IntersectionObserver(function(entries, observer) {
      entries.forEach(function(video) {
        if (video.isIntersecting) {
          for (var source in video.target.children) {
            var videoSource = video.target.children[source];
            if (typeof videoSource.tagName === "string" && videoSource.tagName === "SOURCE") {
              videoSource.src = videoSource.dataset.src;
            }
          }

          video.target.load();
          video.target.classList.remove("lazy");
          lazyVideoObserver.unobserve(video.target);
        }
      });
    });

    lazyVideos.forEach(function(lazyVideo) {
      lazyVideoObserver.observe(lazyVideo);
    });
  }
});
</script>

<script>
    const videoCompareLeft = document.getElementById("video-compare-left");
    const videoCompareRight = document.getElementById("video-compare-right");
    const videoExitButton = document.getElementById("video-exit-button");
    const videoViewExitButton = document.getElementById("video-view-exit-button");
    const videoView = document.getElementById("tnb-video-view");
    const videoLabel = document.getElementById("compare-label-video");
    
    function mobileCheck() {
        var windowSize = window.matchMedia("(max-width: 767px)");
        if(windowSize.matches) {
  	         return true;
        } else {
  	         return false;
        }
    }
    
    function compareButtonVideo() {
        if(mobileCheck()) {
            videoCompareLeft.src = this.dataset.beforemobile;
            videoCompareRight.src = this.dataset.aftermobile;
            
            videoCompareLeft.type = "video/mp4";
            videoCompareRight.type = "video/mp4";
        } else {
            videoCompareLeft.src = this.dataset.before;
            videoCompareRight.src = this.dataset.after;
            
            videoCompareLeft.type = "video/webm";
            videoCompareRight.type = "video/webm";
        }
        
        videoLabel.innerHTML = this.dataset.label;
        
        videoCompareLeft.play();
        videoCompareRight.play();
    }
    
    function playButtonVideo() {
    		if(mobileCheck()) {
            videoView.src = this.dataset.videomobile;
            videoView.type = "video/mp4";
        } else {
            videoView.src = this.dataset.video;
            videoView.type = "video/webm";
        }
        
        videoView.play();
    }
    
    function initVideoCompareButtons() {
        //var compareButtons = document.getElementsByClassName("tnb-button-small-compare-video");
        var compareButtons = document.querySelectorAll(".tnb-button-small-compare-video,.trigger-video-compare");
        
        if(compareButtons.length > 0){
       		for(let i = 0; i < compareButtons.length; i++) {
            	compareButtons[i].addEventListener("click",compareButtonVideo);
        	}
        }
    }
    
    function initVideoViewButtons() {
    		var videoViewButtons = document.getElementsByClassName("tnb-play");
        
        if(videoViewButtons.length > 0){
        	for(let i = 0; i < videoViewButtons.length; i++) {
            	videoViewButtons[i].addEventListener("click",playButtonVideo);
        	}
        }
    }
    
    function initVideoViewButtonsSmall() {
    		var videoViewButtons = document.getElementsByClassName("tnb-play-link");
        
        if(videoViewButtons.length > 0){
        	for(let i = 0; i < videoViewButtons.length; i++) {
            	videoViewButtons[i].addEventListener("click",playButtonVideo);
        	}
        }
    }
    
    function pauseCompareVideos() {
        videoCompareLeft.pause();
        videoCompareRight.pause();
    }
    
    function pauseViewVideo() {
    		videoView.pause();
    }
    
    if(videoExitButton != null) {videoExitButton.addEventListener("click",pauseCompareVideos);}
    if(videoViewExitButton != null) {videoViewExitButton.addEventListener("click",pauseViewVideo);}
    
    initVideoCompareButtons();
    initVideoViewButtons();
    initVideoViewButtonsSmall();
